# Discord Bot Configuration
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_application_id_here
GUILD_ID=your_guild_id_here

# MongoDB Configuration (shared for whitelist and ticket systems)
MONGO_URI=mongodb://localhost:27017/minecraft-bot
# Alternative: MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/database

# Whitelist System Configuration
CONSOLE_CHANNEL_ID=your_console_channel_id
FORMS_CHANNEL_ID=your_forms_channel_id
PUBLIC_RESULTS_CHANNEL_ID=your_results_channel_id
WHITELISTED_ROLE_ID=your_whitelisted_role_id
STAFF_ROLE_ID=your_staff_role_id
WELCOME_CHANNEL_ID=your_welcome_channel_id

# DiscordSRV Integration
CONSOLE_COMMAND_PREFIX=!
WHITELIST_COMMAND_TEMPLATE=whitelist add {ign}

# Ticket System Configuration (Optional - License-based features)
LICENSE_KEY=shotdevs-your-license-key-here
LICENSE_API_BASE_URL=https://shotdevs.live

# Application Settings
NODE_ENV=development
LOG_LEVEL=info
DEFAULT_COOLDOWN=300000
